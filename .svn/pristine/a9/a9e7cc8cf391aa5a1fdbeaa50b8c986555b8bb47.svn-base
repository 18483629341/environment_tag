html,
body,
div,
span,
a,
ul,
li,
p,
img,
iframe,
sub,
h1,
h2,
h3,
h4,
h5,
h6{
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}

html{
	width: 100%;
	height: 100%;
}

body {
	font-family: "microsoft yahei";
	font-size: 14px;
	line-height: 24px;
	width: 100%;
	height: 100%;
	background-color: #f4f4f4;
}

h1,
h2,
h3,
h4,
h5,
h6 {
	font-size: 14px;
	font-weight: normal;
	color: inherit;
}

ul,
ol,
li {
	list-style: none;
	padding: 0;
	margin: 0;
}

.containter-title{
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 65px;
	padding: 0 23px;
	background-color: #061533;
	z-index: 999;
}
.containter-title img{
	margin-top:14px;
}

.containter-title-nav{
	position: absolute;
	top: 0;
	right: 155px;
}
.containter-title-nav li{
	position: relative;
	float: left;
	padding: 0 37px;
	line-height: 65px;
	color: #5f7aa4;
	font-size: 16px;
	font-weight: 600;
	cursor: pointer;
}
.containter-title-nav li:after{
	position: absolute;
    content: '';
    bottom: 4px;
    left: calc(50% - 31px);
    width: 62px;
    height: 3px;
    background-color: #2edbff;
    border-radius: 10px;
    display: none;
}
.containter-title-nav li:first-child:after{
	left: auto;
}

.containter-title-nav li.active:after{
	display: block;
}

.containter-title-nav span{
	vertical-align: middle;
}

.containter-title-nav li img{
	margin-top: 0;
	margin-right: 8px;
    vertical-align: middle;
}
.containter-title-nav li.active{
	font-weight: 600;
	color: #2edbff;
}

.containter-title-nav li .nav-img-on{
	display: none;
}
.containter-title-nav li.active .nav-img-on{
	display: inline;
}
.containter-title-nav li.active .nav-img-off{
	display: none;
}


.containter-title-role{
	float: right;
    line-height: 65px;
    color: #5f7aa4;
    cursor: pointer;
}
.containter-title-role img{
	width: 12px;
}
.containter-left{
	position: fixed;
    left: 0;
    top: 64px;
	width: 190px;
	height:calc(100% - 65px);
	background-color: #0e2147;
	z-index: 999;
}
.containter-left ul{
	padding: 9px 16px 0 14px;
}

.containter-left ul li{
	color: #5f7aa4;
	margin-bottom: 20px;
	background: url(../img/10.png) no-repeat right;
	cursor: pointer;
}
.containter-left ul>li.active{
	background: url(../img/09_03.png) no-repeat right;
	color: #2edbff;
}

.containter-left ul>li img{
	margin-right: 8px;
    vertical-align: middle;
}
.containter-left ul>li img.li-img-on{
	display: none;
}
.containter-left ul>li.active img.li-img-on{
	display: inline;
}
.containter-left ul>li.active img.li-img-off{
	display: none;
}

.containter-right{
	margin-left: 190px;
    width: calc(100% - 190px);
    height: calc(100% - 65px);;
	/*width: 1730px;*/
	/*height: 1015px;*/
	padding: 0 20px;
	background-color: #f4f4f4;
}
.right-title{
	padding: 15px 0;
	font-size: 18px;
	color: #1d1d1d;
}

.right-title img{
	vertical-align: middle;
}

