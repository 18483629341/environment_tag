html,
body,
div,
span,
a,
ul,
li,
p,
img,
h1,
h2,
h3,
h4,
h5,
h6{
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}

html{
	width: 100%;
	height: 100%;
}

body {
	font-family: "microsoft yahei";
	font-size: 14px;
	line-height: 24px;
	width: 100%;
	height: 100%;
}

h1,
h2,
h3,
h4,
h5,
h6 {
	font-size: 14px;
	font-weight: normal;
	color: inherit;
}

ul,
ol,
li {
	list-style: none;
	padding: 0;
	margin: 0;
}

.containter{
	width: 1920px;
	height: 1080px;
}
.containter-title{
	position: relative;
	width: 100%;
	height: 65px;
	padding: 0 23px;
	background-color: #061533;
}
.containter-title img{
	margin-top:14px;
}

.containter-title-nav{
	position: absolute;
	top: 0;
	right: 155px;
}
.containter-title-nav li{
	float: left;
	padding: 0 37px;
	line-height: 65px;
	color: #5f7aa4;
	font-size: 16px;
	font-weight: 600;
	cursor: pointer;
}

.containter-title-nav span{
	padding-bottom: 16px;
}

.containter-title-nav li img{
	margin-top: 0;
	margin-right: 8px;
    vertical-align: inherit;
}
.containter-title-nav li.active{
	font-weight: 600;
	color: #2edbff;
}
.containter-title-nav li.active span{
	border-bottom: 3px solid #2edbff;
}

.containter-title-nav li .nav-img-on{
	display: none;
}
.containter-title-nav li.active .nav-img-on{
	display: inline;
}
.containter-title-nav li.active .nav-img-off{
	display: none;
}


.containter-title-role{
	float: right;
    line-height: 65px;
    color: #5f7aa4;
    cursor: pointer;
}
.containter-title-role img{
	width: 12px;
}
.containter-left{
	float: left;
	width: 190px;
	height:1015px;
	margin-top: -1px;
	background-color: #0e2147;
}
.containter-left ul{
	padding: 9px 16px 0 14px;
}

.containter-left ul li{
	color: #5f7aa4;
	margin-bottom: 20px;
	background: url(../img/10.png) no-repeat right;
	cursor: pointer;
}
.containter-left ul>li.active{
	background: url(../img/09_03.png) no-repeat right;
	color: #2edbff;
}

.containter-left ul>li img{
	margin-right: 8px;
    vertical-align: middle;
}
.containter-left ul>li img.li-img-on{
	display: none;
}
.containter-left ul>li.active img.li-img-on{
	display: inline;
}
.containter-left ul>li.active img.li-img-off{
	display: none;
}

.containter-right{
	float: left;
	width: 1730px;
	height: 1015px;
	padding: 0 20px;
	background-color: #f4f4f4;
}
.right-title{
	padding: 15px 0;
	font-size: 18px;
	color: #1d1d1d;
}

.right-title img{
	vertical-align: middle;
}

.key-features{
	width: 100%;
	height: 277px;
	padding: 14px 14px 0;
	background-color: #ffffff;
	border-radius: 6px;
}
.key-features ul{
	float: left;
}
.large-household{
	width: 1035px;
}
.large-household li:first-child{
	background: url(../img/01_03.png) no-repeat;
}
.large-household li:nth-child(2){
	background: url(../img/01_05.png) no-repeat;
}
.large-household li:nth-child(3){
	background: url(../img/01_07.png) no-repeat;
}
.large-household li:nth-child(4){
	background: url(../img/012_20.png) no-repeat;
}
.large-household li:nth-child(5){
	background: url(../img/012_21.png) no-repeat;
}
.large-household li:nth-child(6){
	background: url(../img/012_22.png) no-repeat;
}

.large-household-box{
	float: left;
	width: 332px;
	height: 118px;
	padding: 23px;
	margin-right: 13px;
	margin-bottom: 13px;
	color: #fff;
	border-radius: 6px;
}
.large-household-box h3{
	padding-bottom: 27px;
	font-size: 20px;
	font-weight: 600;
}
.large-household-box p{
	font-size: 18px;
}

.feature-labels{
	width: 626px;
}
.feature-labels>li{
	width: 100%;
	height: 119px;
	margin-bottom: 11px;
	padding: 11px 16px 16px 100px;
	border-radius: 4px;
	border: solid 1px #e9e9e9;
}
.feature-labels>li:first-child{
	background: url(../img/02_14.png) no-repeat 16px 16px;
}
.feature-labels>li:last-child{
	background: url(../img/02_17.png) no-repeat 16px 16px;
}

.feature-labels>li h3{
	margin-bottom: 8px;
	font-size: 16px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1px;
	color: #424242;
}
.labels li{
	float: left;
	padding: 0 10px;
	margin-right: 8px;
	margin-bottom: 8px;
	letter-spacing: 1px;
	line-height: 25px;
	color: #fff;
	border-radius: 4px;
}
.positive-label li{
	background-color: #44b5ae;
}
.negative-label li{
	background-color: #f2565c;
}

.feature-statistics{
	padding: 20px;
	height: 602px;
	background-color: #fff;
	border-radius: 6px;
}
.statistics-title h3{
	font-size: 16px;
	color: #1d1d1d;
}
.statistics-title h5{
	font-size: 12px;
	color: #b4b4b4;
}

.word-cloud{
	position: relative;
	float: left;
	height: 562px;
	width: 1105px;
}
.word-check-boxs{
	/*position: absolute;*/
	float: left;
	width: 215px;
	height: 243px;
	margin-top: 20px;
	padding: 0 17px;
	color: #1d1d1d;
	border-radius: 4px;
	border: solid 1px #eee;
}
.word-check-boxs li{
	line-height: 39px;
	border-bottom: 1px solid #eee;
	background: url(../img/08_38.png) no-repeat right;
	cursor: pointer;
}
.word-check-boxs li:last-child{
	border-bottom: none;
}
.word-check-boxs li.active{
	background: url(../img/08_35.png) no-repeat right;
}
.word-cloud-chart{
	float: left;
	width: 890px;
	height: 517px;;
}
.label-rank{
	float: left;
	height: 562px;
	width: 545px;
}

.ranking-table{
	width: 100%;
	color: #1d1d1d;
}
.ranking-table td{
	line-height: 35px;
	text-align: center;
}
.ranking-table td img{
	vertical-align: middle;
}
.ranking-table tr:first-child{
	font-weight: 600;
}
.ranking-table tr:nth-child(even){
	background-color: #f6f6f6;
}
.ranking-table tr td:first-child{
	width: 135px;
}
.ranking-table tr td:nth-child(2){
	display: block;
	width: 260px;
	text-align: left;
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
.ranking-table tr td:last-child{
	text-align: right;
	padding-right: 36px;
}

